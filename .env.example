# Configurações do Banco de Dados
POSTGRES_DB=peopleconnect
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Configurações da Aplicação
ASPNETCORE_ENVIRONMENT=Production
JWT_SECRET=your_jwt_secret_key_here

# URLs - Usado pelo docker-compose para construir a imagem do frontend e configurar o CORS no backend.
# A VITE_API_URL é injetada no build do frontend.
# A FRONTEND_URL é usada pelo backend para a política de CORS em produção.
VITE_API_URL=http://localhost/api
FRONTEND_URL=http://localhost # Ex: https://www.seusite.com

# Configurações de Deploy
COMPOSE_PROJECT_NAME=peopleconnect
